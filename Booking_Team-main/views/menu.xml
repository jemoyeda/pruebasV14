<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- This Menu Item will appear in the Upper bar, that's why It needs NO parent or action -->
    <menuitem 
    id="booking_menu" 
    parent="sale.sale_menu_root"
    name="Booking" 
    sequence="10"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem 
    id="booking_order_menu_categ" 
    name="Booking Order"
    parent="booking_menu"
    action="booking_order_action"
    sequence="10"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem 
    id="work_order_menu_categ" 
    name="Work Order" 
    parent="booking_menu_root"
    action="work_order_action" 
    sequence="10"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem 
    id="service_team_menu_categ" 
    name="Service Team" 
    parent="booking_menu_root"
    action="service_team_action"
    sequence="10"/>

   
    

</odoo>